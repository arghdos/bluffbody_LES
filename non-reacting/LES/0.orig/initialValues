FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "0";
    object      initialValues;
}


// a collection of initial values, importable to other files

#include "../system/meshDims"

// operating pressure, 1 bar
Pref  		100000;
// mass fraction of N2 of air, for non-reacting
N2_init		0.7669924994868982;
// mass fraction of O2 of air, for non-reacting
O2_init		0.23300750051310173;
// inlet temperature
Tref        288;
// inlet velocity
Ubulk 		16.6;
// specific heat ratio
gamma		1.3999110177501861;
// mass flow rate (kg/s)
mdot 		0.19198781051276775;
// inlet density (kg/m3)
rho_inlet 	1.2047427868522074;
// residence time, in seconds
t_res		#calc "($L_total * $convertToMeters) / $Ubulk";
