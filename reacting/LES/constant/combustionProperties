FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel  pasrIPM;

active  true;

EDCCoeffs
{
    version v2005;
}

// cmix from
// http://www.tfd.chalmers.se/~hani/kurser/OS_CFD_2014/Lorenzo%20Bartolucci/Doc/LorenzoBartolucciTutorial.pdf
// http://files.nequam.se/thesis.pdf
pasrIPMCoeffs
{
    Cmix                0.1;
    turbulentReaction   on;
}



