FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          chemistryProperties;
}

/*

Chemistry properties for reacting case

*/

chemistryType
{
    chemistrySolver ode;
    chemistryThermo psi;
    TDAC            off;
    reduction       off;
}

chemistry       on;

initialChemicalTimeStep 1e-07;

odeCoeffs
{
    solver          rodas34;
    absTol          1e-10;
    relTol          1e-06;
}

