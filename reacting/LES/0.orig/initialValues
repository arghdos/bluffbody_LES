FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "0";
    object      initialValues;
}


// a collection of initial values, importable to other files

#include "../system/meshDims"

// operating pressure, 1 bar
Pref  		100000;
// equivalence ratio
phi         1.0;
// mass fraction of CH4, for an equivalence ratio of 1.0
CH4_init    0.045706816557972915;
// mass fraction of N2 of air, for reacting
N2_init		0.7319357140115094;
// mass fraction of O2 of air, for reacting
O2_init		0.22235746943051762;
// inlet temperature
Tref        288;
// inlet velocity
Ubulk 		17.2;
// specific heat ratio
gamma		1.3888546897695568;
// mass flow rate (kg/s)
mdot 		0.1905313363453628;
// inlet density (kg/m3)
rho_inlet 	1.153896174572207;
// residence time, in seconds
t_res		#calc "($L_total * $convertToMeters) / $Ubulk";
